From b14c587ffd9b3bd1d15ecf31edaa9b9fc6f3f992 Mon Sep 17 00:00:00 2001
From: Marcus Comstedt <marcus@mc.pp.se>
Date: Mon, 24 Sep 2018 18:11:23 +0200
Subject: [PATCH 1/5] build_ffmpeg: Recognize ppc64

---
 .../ffmpeg/chromium/scripts/build_ffmpeg.py            | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/src/3rdparty/chromium/third_party/ffmpeg/chromium/scripts/build_ffmpeg.py b/src/3rdparty/chromium/third_party/ffmpeg/chromium/scripts/build_ffmpeg.py
index 3232d4e13..3bfbe2991 100755
--- a/src/3rdparty/chromium/third_party/ffmpeg/chromium/scripts/build_ffmpeg.py
+++ b/src/3rdparty/chromium/third_party/ffmpeg/chromium/scripts/build_ffmpeg.py
@@ -37,7 +37,7 @@ ARCH_MAP = {
     'android': ['ia32', 'x64', 'arm-neon', 'arm64'],
     'linux': [
         'ia32', 'x64', 'mipsel', 'mips64el', 'noasm-x64', 'arm', 'arm-neon',
-        'arm64', 'ppc64'
+        'arm64', 'ppc64', 'ppc64le'
     ],
     'mac': ['x64', 'arm64'],
     'win': ['ia32', 'x64', 'arm64'],
@@ -139,7 +139,7 @@ def DetermineHostOsAndArch():
     host_arch = 'mips64el'
   elif platform.machine().startswith('arm'):
     host_arch = 'arm'
-  elif platform.machine() == 'ppc64le':
+  elif platform.machine() == 'ppc64' or platform.machine() == 'ppc64le':
     host_arch = 'ppc64'
   else:
     return None
@@ -822,9 +822,9 @@ def ConfigureAndBuild(target_arch, target_os, host_os, host_arch, parallel_jobs,
             '--extra-cflags=--target=mips64el-linux-gnuabi64',
             '--extra-ldflags=--target=mips64el-linux-gnuabi64',
         ])
-    elif target_arch == 'ppc64':
+    elif target_arch == 'ppc64' or target_arch == 'ppc64le':
       configure_flags['Common'].extend([
-        '--arch=ppc64le',
+        '--arch='+target_arch,
         '--extra-cflags=-mcpu=power9'
       ])
     else:
@@ -852,7 +852,7 @@ def ConfigureAndBuild(target_arch, target_os, host_os, host_arch, parallel_jobs,
     # typically be the system one, so explicitly configure use of Clang's
     # ld.lld, to ensure that things like cross-compilation and LTO work.
     # This does not work for ia32 and is always used on mac.
-    if target_arch != 'ia32' and target_os != 'mac':
+    if target_arch != 'ia32' and target_os != 'mac' and target_arch != 'ppc64':
       configure_flags['Common'].append('--extra-ldflags=-fuse-ld=lld')
 
   # Should be run on Mac, unless we're cross-compiling on Linux.
-- 
2.41.0


